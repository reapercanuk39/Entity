[Unit]
Description=Queen daemon (local)
After=network.target

[Service]
Type=simple
ExecStart=/usr/sbin/python3 /root/Entity/Queen.py --config /root/Entity/queen_config.json
WorkingDirectory=/root/Entity
Restart=on-failure
RestartSec=10
Environment=ALLOW_LIVE=1

[Install]
WantedBy=multi-user.target
